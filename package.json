{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "analyze": "nuxt analyze",
    "eslint": "npx eslint . --ext .vue,.js,.ts,.tsx,.jsx,.cjs --ignore-path .eslintignore",
    "eslint:fix": "npx eslint . --ext .vue,.js,.ts,.tsx,.jsx,.cjs --fix --ignore-path .eslintignore",
    "prettier": "pnpm exec prettier . --write --ignore-path --ignore-path .eslintignore",
    "stylelint": "stylelint \"./**/*.{css,less,scss,sass,vue,html}\" --fix --cache --cache-location node_modules/.cache/stylelint/ --ignore-path .gitignore"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.51.0",
    "eslint-plugin-vue": "^9.17.0",
    "nuxt": "^3.8.0",
    "postcss": "^8.4.31",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "prettier": "3.0.3",
    "stylelint": "^15.11.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-less": "^2.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-order": "^6.0.3",
    "vue": "^3.3.4",
    "vue-eslint-parser": "^9.3.2",
    "vue-router": "^4.2.5"
  }
}
